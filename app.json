{
  "expo": {
    "name": "Gochi",
    "slug": "Gochi",
    "version": "1.0.0",
    "platform": [
      "ios",
      "andriod"
    ],
    "jsEngine": "hermes",
    "orientation": "default",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#e63c4b"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/splash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gochi_s.Gochi"
    },
    "web": {
      "favicon": "./app/assets/icon.png",
      "bundler": "metro"
    },
    "sdkVersion": "51.0.0",
    "extra": {
      "eas": {
        "projectId": "f4a888f6-7fee-453b-adee-379d3e7d2df6"
      },
      "bugsnag": {
        "apiKey": "4378d4ae1fcecbbf8f6b52453cd13dab"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    }
}